{
  "name": "subquery-starter-terra",
  "version": "0.0.1",
  "description": "This project can be use as a starting point for developing your SubQuery project",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/subql build",
    "prepack": "rm -rf dist && npm build",
